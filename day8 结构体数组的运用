#include<iostream>
#include<string>

using namespace std;

struct student
{
	string name;
	int age;
	double score;
};

int main()
{
	student arr[3] = {
		{"张三",15,50},
		{"alice",17,100},
		{"natsumi",18,120}
	};

	arr[0].name = "tsukinomito";

	for(int i = 0;i < 3; i++)
	{
		cout << "姓名：" << arr[i].name << "  年龄：" << arr[i].age << "  成绩:" << arr[i].score << "\t";
		cout << endl;
	}
	return 0;
}
